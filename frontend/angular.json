{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "projectType": "application",
      "schematics": {
        "@ionic/angular-toolkit:page": {
          "styleext": "scss",
          "standalone": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.config.js"
            },
            "outputPath": "wwww",
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "tsconfig.web.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "src/global.scss",
              "src/theme/variables.scss"
            ],
            "scripts": [],
            "main": "src/main.web.ts"
          },
          "configurations": {
            "web-production": {
              "outputPath": "www/web",
              "index": "src/index.web.html",
              "main": "src/main.web.ts",
              "tsConfig": "tsconfig.web.json",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets",
                  "output": "assets"
                },
                {
                  "glob": "manifest.json",
                  "input": "src",
                  "output": ""
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "40kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "extractLicenses": true,
              "sourceMap": false
            },
            "web-development": {
              "outputPath": "www/web",
              "index": "src/index.web.html",
              "main": "src/main.web.ts",
              "tsConfig": "tsconfig.web.json",
              "fileReplacements": [],
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets",
                  "output": "assets"
                },
                {
                  "glob": "manifest.json",
                  "input": "src",
                  "output": ""
                }
              ],
              "optimization": false,
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false
            },
            "mobile-production": {
              "outputPath": "www/mobile",
              "index": "src/index.mobile.html",
              "main": "src/main.mobile.ts",
              "tsConfig": "tsconfig.mobile.json",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "5kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "extractLicenses": true,
              "sourceMap": false
            },
            "mobile-development": {
              "outputPath": "www/mobile",
              "index": "src/index.mobile.html",
              "main": "src/main.mobile.ts",
              "tsConfig": "tsconfig.mobile.json",
              "fileReplacements": [],
              "optimization": false,
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false
            },
            "ci": {
              "progress": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "web-production": {
              "buildTarget": "app:build:web-production"
            },
            "web-development": {
              "buildTarget": "app:build:web-development"
            },
            "mobile-production": {
              "buildTarget": "app:build:mobile-production"
            },
            "mobile-development": {
              "buildTarget": "app:build:mobile-development"
            }
          },
          "defaultConfiguration": "mobile-development"
        },
        "extract-i18n": {
          "builder": "@angular-builders/custom-webpack:extract-i18n",
          "options": {
            "buildTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "configPath": "./jest.config.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@ionic/angular-toolkit"
    ],
    "analytics": false
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    },
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
